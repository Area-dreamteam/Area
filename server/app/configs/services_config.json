{
  "gmail": {
    "auth": {
      "type": "oauth2",
      "auth_url": "https://google.com",
      "token_url": "https://google.com"
    },
    "api": {
      "root_url": "https://google.com"
    },
    "polling": {
      "enabled": true,
      "interval": 60
    },
    "webhooks": {
      "supported": false
    }
  },
  "github": {
    "auth": {
      "type": "oauth2",
      "auth_url": "https://github.com",
      "token_url": "https://github.com"
    },
    "api": {
      "root_url": "https://github.com"
    },
    "polling": {
      "enabled": false
    },
    "webhooks": {
      "supported": true,
      "path": "/webhooks/github"
    }
  }
}
